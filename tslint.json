{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-eslint-rules"
  ],
  "jsRules": {},
  "rules": {
    "indent": [true, "spaces", 2],
    "ter-indent": [true, 2],
    "align": [true, "parameters", "statements", "members"],
    "semicolon": [true, "never", "ignore-interfaces"],
    "quotemark": [false],
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "trailing-comma": false
  },
  "rulesDirectory": []
}
